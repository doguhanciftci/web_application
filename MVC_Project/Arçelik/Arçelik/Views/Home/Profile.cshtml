
@{
    ViewData["Title"] = "Profile";
}
@model Arçelik.Models.Applicant

<br />
<h1 style="text-align:center">Profile</h1>
<br />
<br />
<h2>Update</h2>
<br />
<br />
<form class="m-1 p-1" method="post">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" type="text" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Fname"></label>
        <input asp-for="Fname" type="text" class="form-control" />
        <span asp-validation-for="Fname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Lname"></label>
        <input asp-for="Lname" type="text" class="form-control" />
        <span asp-validation-for="Lname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LoginModel.Username"></label>
        <input asp-for="LoginModel.Username" type="text" class="form-control" />
        <span asp-validation-for="LoginModel.Username" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LoginModel.Password"></label>
        <input asp-for="LoginModel.Password" type="text" class="form-control" />
        <span asp-validation-for="LoginModel.Password" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Age"></label>
        <input asp-for="Age" type="text" class="form-control" />
        <span asp-validation-for="Age" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Phone"></label>
        <input asp-for="Phone" type="text" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="AddedOn"></label>
        <input asp-for="AddedOn" type="text" readonly class="form-control" asp-format="{0:d}" />
    </div>
    <div>
        <input type="submit" name="cmd" value="Update" class="btn btn-primary" />
        <input type="submit" name="cmd" value="Delete" class="btn btn-warning" />
    </div>

    <!-- <button type="submit" name="submit" class="btn btn-primary">Submit</button>  -->
</form>
<script src="/lib/jquery/dist/jquery.min.js"></script>
<script src="/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js"></script>