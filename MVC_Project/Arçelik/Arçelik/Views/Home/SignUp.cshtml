
@{
    ViewData["Title"] = "SignUp";
}
@model Arçelik.Models.Applicant


<h2>SignUp</h2>

<br />
<br />
<form class="m-1 p-1" method="post">
    <div class="form-group">
        <label asp-for="Fname">First Name</label>
        <input asp-for="Fname" type="text" class="form-control" required />
        <span asp-validation-for="Fname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Lname">Last Name</label>
        <input asp-for="Lname" type="text" class="form-control" required />
        <span asp-validation-for="Lname" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LoginModel.Username">Username</label>
        <input asp-for="LoginModel.Username" type="text" class="form-control" required />
        <span asp-validation-for="LoginModel.Username" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LoginModel.Password">Password</label>
        <input asp-for="LoginModel.Password" type="password" class="form-control" required />
        <span asp-validation-for="LoginModel.Password" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Age">Age</label>
        <input asp-for="Age" type="text" class="form-control" />
        <span asp-validation-for="Age" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Phone">Phone</label>
        <input asp-for="Phone" type="text" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ExtraInfo">Extra</label>
        <input asp-for="ExtraInfo" type="text" class="form-control" />
        <span asp-validation-for="ExtraInfo" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
<script src="/lib/jquery/dist/jquery.min.js"></script>
<script src="/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js"></script>

